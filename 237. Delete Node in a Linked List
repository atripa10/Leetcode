# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution(object):
    def deleteNode(self, node):
        """
        :type node: ListNode
        :rtype: void Do not return anything, modify node in-place instead.
        """
        back = node
        front = node.next
        while True:
            temp = back.val
            back.val = front.val
            front.val = temp
            if front.next == None:
                back.next = None
                break
            back = back.next
            front = front.next
